from os import *
from sys import *
from collections import *
from math import *

## Read input as specified in the question.
## Print output as specified in the question.
n=int(input())
i=1

while i<=n:
    char=chr(ord('A')+n-i)
    j=1
    while j<=i:
        print(chr(ord(char)+j-1),end='')
        j+=1
    print()
    i+=1
