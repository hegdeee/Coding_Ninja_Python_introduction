

def checkPalindrome(num):
    rev = 0
    temp = num
    while num != 0:
        rem = num % 10
        rev = rev * 10 + rem
        num = num // 10

    if rev == temp:
        return 1


pass

num = int(input())
isPalindrome = checkPalindrome(num)
if (isPalindrome):
    print('true')
else:
    print('false')
